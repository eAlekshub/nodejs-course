{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },

  "extends": ["plugin:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint/eslint-plugin"],

  "rules": {
    "semi": "error",
    "@typescript-eslint/explicit-function-return-type": "error"
  }
}
